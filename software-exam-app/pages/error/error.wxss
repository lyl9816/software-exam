  margin-left: 32%;

  border-radius: 50%;  
  border: 2px solid #f0ada0;  
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);  
  text-align: center;
  
}
.font{ 
  font-size:50px;
  color: #d42517;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  

 
}
